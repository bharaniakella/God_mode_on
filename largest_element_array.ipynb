        "import numpy as np\n",
        "\n",
        "arr=np.array([3,8,2,5,9,6,4])"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def max_array(arr):\n",
        "  max=arr[0]\n",
        "  for i in range(1,len(arr)):\n",
        "    if(arr[i]>max):\n",
        "      max=arr[i]\n",
        "  return max"
      ],
      "metadata": {
